{
    "name": "coingate/magento2-plugin",
    "type": "magento2-module",
    "description": "Bitcoin integration for Magento 2 via CoinGate",
    "version": "1.0.0",
    "keywords": [
      "coingate",
      "bitcoin",
      "merchant",
      "magento"
    ],
    "homepage": "https://coingate.com",
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "CoinGate and contributors",
            "homepage": "https://github.com/coingate/magento2-plugin/graphs/contributors"
        }
    ],
    "require": {
        "coingate/coingate-php": "*"
    },
    "extra": {
        "map": [
            [
                "*",
                "CoinGate/Merchant"
            ]
        ]
    },
    "autoload": {
        "files": [ "registration.php" ],
        "psr-4": {
            "CoinGate\\Merchant\\": ""
        }
    }
}
